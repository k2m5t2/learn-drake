    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug data_provider",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/arm_teleop/data_provider.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/arm_teleop",
        },
        {
          "name": "Debug examples.mp_teleop_demo",
          "type": "debugpy",
          "request": "launch",
          "module": "examples.mp_teleop_demo",
          "console": "integratedTerminal",
          "cwd": "${workspaceFolder}/arm_teleop"
      }
    ]
}
